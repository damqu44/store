{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      { "src": "/", "dest": "/index.html" },
      { "src": "/product", "dest": "/product.html" },
      { "src": "/search", "dest": "/search.html" },
      { "src": "/cart", "dest": "/cart.html" },
      { "src": "/register", "dest": "/register.html" },
      { "src": "/login", "dest": "/login.html" },
      { "src": "/account", "dest": "/account.html" },
      { "src": "/transaction", "dest": "/transaction.html" },
      { "src": "/orders", "dest": "/orders.html" },
      { "src": "/(.*)", "dest": "/error.html" }
    ]
  }
  